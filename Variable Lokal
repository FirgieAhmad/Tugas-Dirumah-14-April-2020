#include <iostream>
using namespace std;
// Tugas Dirumah Pada Tanggal 14 Maret 2020 == Membuat Variable Lokal dan Variable Global
// Nama : Firgie Ahmad == NIM ETE10190123

int contohFunction ()
{
	//deklarasi variable
	int a = 30;
	return a;
	//variable dihancurkan /dihapus dari alamat memori 
}

int contohFunction2()
{
	int b = 50;
	return b;
	//variable dihancurkan /dihapus dari alamat memori 
}

int main ()
{
		int a = 15;
		cout << " ini variable lokal dari fungsi utama " << a << endl;
		cout << " ini variable lokal dari fungsi contohFunction " << contohFunction();
		cout << endl;
		int b = 25;
		cout << " ini variable lokal dari fungsi kedua " << b << endl;
		cout << " ini variable lokal dari fungsi contohFunction2 " << contohFunction2();
		cout << endl;
}
